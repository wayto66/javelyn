{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "generateOptions": {
    "baseDir": "src/modules"
  },
  "watchAssets": {
    "ignore": ["./wwebjs_auth/**", "./wwebjs_cache/**"]
  }
}
